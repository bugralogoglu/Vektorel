package KalitimMuhendis;

public class Islem {


	
	public static void main(String[] args) {
		InsaatMuhendisi imuhendis = new InsaatMuhendisi();
		BilgisayarMuhendisi bmuhendis = new BilgisayarMuhendisi();
		
		bmuhendis.mat();
		bmuhendis.yazilim();
		imuhendis.insaatbil();
		imuhendis.mat();
		bmuhendis.setDiploma("Gazi üniversitesi");
		bmuhendis.getDiploma();
		imuhendis.getDiploma();
	}
	
}
