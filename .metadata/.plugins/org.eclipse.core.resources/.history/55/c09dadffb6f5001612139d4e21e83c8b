package Fonksiyon;
import java.util.Scanner;
public class Faktoriyel {
public static void faktoriyel(int a){
	for (int i = 1; i<=a;i++){
		i = i * i;
	}
	System.out.println(i);
}
public static void main(String[] args) {
	Scanner oku = new Scanner (System.in);
	int fak = oku.nextInt();
	faktoriyel(fak);
}

}
