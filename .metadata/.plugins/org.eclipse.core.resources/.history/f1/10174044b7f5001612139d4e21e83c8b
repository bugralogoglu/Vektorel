package Fonksiyon;
import java.util.Scanner;
public class Faktoriyel {
public static void faktoriyel(int a){
	int sonuc = 1;
	for (int i = 1; i<=a;i++){
		sonuc = sonuc * i;
	}
	System.out.println(sonuc);
}
public static void main(String[] args) {
	Scanner oku = new Scanner (System.in);
	int fak = oku.nextInt();
	faktoriyel(fak);
}

}
