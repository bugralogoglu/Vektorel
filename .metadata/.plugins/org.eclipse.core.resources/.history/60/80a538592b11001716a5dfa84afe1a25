package SporPackage;

public class Islem {
	
	
	void EkranYazdir(Spor[] arraySpor){
		for(int i = 0; i<arraySpor.length;i++){
			System.out.println(arraySpor[i].turu);
		}	
	}
	
	public static void main(String[] args) {
		
		Spor basketbol = new Spor();
		basketbol.turu = "Basketbol";
		Spor futbol = new Spor();
		futbol.turu = "Futbol";
		Spor voleybol = new Spor();
		voleybol.turu = "Voleybol";
		
		
		Spor[] arraySpor = new Spor[3];
		arraySpor[0] = basketbol;
		arraySpor[1] = futbol;
		arraySpor[2] = voleybol;

	
	}

	
	

}
